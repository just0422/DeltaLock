<%= form_for @key, url: {controller: :upload, action: :create},  html: { multipart: true }, remote: true do |f| %>
	<%= f.label :keyfile %>
	<%= f.file_field :keyfile %>
	<%= f.submit "Save", class: "btn btn-primary" %>
	<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
<% end %>
