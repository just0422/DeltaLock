<% if @purchasers_list.length > 0 %>
    <h1 class="search-table-header">Purchasers</h1>
    <table class="table bordered centered responsive-table">
        <thead class="thead-inverse">
            <tr>
                <% Purchaser.column_names.each do |title| %>
					<%= render '/search/tables/header', title:title %>
                <% end %>
            </tr>
        </thead>
        <tbody>
            <% @purchasers_list.each do |entry| %>
				<tr class="search-row" data-href="<%= show_entry_path("purchaser", entry["id"]) %>">
					<%= render '/search/tables/body', entryClass: Purchaser, entry: entry, classString: "purchaser"  %>
                </tr>
            <% end %>
        </tbody>
    </table>
<% end %>
