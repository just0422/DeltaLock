<h1>KeyUpload#index</h1>
<%= form_for @key, url: {controller: :upload, action: :create},  html: { multipart: true }, remote: true do |f| %>
	<%= f.label :keyfile %>
	<%= f.file_field :keyfile %>

	<%= f.radio_button :keys, "key", :checked => true %>
	<%= f.label :keys %>
	<%= f.radio_button :end_user, "end_user"%>
	<%= f.label :end_users %>


	<%= f.submit "Upload", class: "btn btn-primary" %>
	<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
<% end %>
<p>Find me in app/views/key_upload/index.html.erb</p>
