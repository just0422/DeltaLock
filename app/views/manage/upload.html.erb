<h1>Upload Successful!</h1>
<table class = "table bordered centered responsive-table show-table">
    <thead class = "thead-inverse">
        <tr>
            <% @class.column_names.each do |title| %>
                <% if ColumnLabels.column?(title) %> 
                    <th><%= ColumnLabels.columnToLabel(title) %></th>
                <% else %> 
                    <th style="display:none;"><%= title %></th>
                <% end %>
            <% end %>
        </tr>
    </thead>
    <tbody>
        <% @result.each do |entry| %>
			<tr class="search-row" data-href="<%= entry_path(@type, entry) %>">
                <% @class.column_names.each do |column| %>
                    <td class="<%= @type + "-" + column %>"
                    <% if ColumnLabels.column?(column) %>
                        >
                    <% else %>
                        style="display:none;">
                    <% end %>
                    <%= entry[column] %>
                    </td>
                <% end %>
            </tr>
        <% end %>
    </tbody>
</table>
        
