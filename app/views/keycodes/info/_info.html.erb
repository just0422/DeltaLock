<div id="<%= params[:category] %>-list">
	<ul class="list-group">
		<% categoryList.column_names.each do |title| %>
			<% if ColumnLabels.column?(title) %>
				<li class="list-group-item">
					<strong><%= title %> : </strong>
					<span class="<%= classString + "=" + title %>"><%= entry[title] %></span>
				</li>
			<% end %>
		<% end %>
	</ul>
	<%= link_to path, remote: true do %>
		<button class="btn">Edit</button>
	<% end %>
</div>
<div id="<%= params[:category] %>-form"></div>

