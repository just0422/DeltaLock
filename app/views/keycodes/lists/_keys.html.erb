<% if @key_codes_list.length > 0 %>
    <h1>Key Codes</h1>
    <table class="table all-tables">
        <thead class="thead-inverse">
            <tr>
                <% Key.column_names.each do |title| %>
					<%= render '/keycodes/tables/header', title:title %>
                <% end %>
            </tr>
        </thead>
        <tbody>
            <% @key_codes_list.each do |entry| %>
                <tr data-link="<%= key_path(entry["id"]) %>">
					<%= render '/keycodes/tables/body', entryClass: Key, entry: entry %>
                </tr>
            <% end %>
        </tbody>
    </table>
<% end %>
