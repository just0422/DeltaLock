<% render 'endusers' %>

<% if @key_codes_list.length > 0 %>
    <h1>Key Codes</h1>
    <table class="table table-striped">
        <thead class="thead-inverse">
            <tr>
                <% Key.column_names.each do |title| %>
                    <th><%= title %></th>
                <% end %>
            </tr>
        </thead>
        <tbody>
            <% @key_codes_list.each do |entry| %>
                <tr data-link="<%= key_path(entry["id"]) %>">
                    <% Key.column_names.each do |column| %>
                        <td><%= entry[column] %></td>
                    <% end %>
                </tr>
            <% end %>
        </tbody>
    </table>
<% end %>


<% if @purchase_orders_list.length > 0 %>
    <h1>Purchase Orders</h1>
    <table class="table table-striped">
        <thead class="thead-inverse">
            <tr>
                <% PurchaseOrder.column_names.each do |title| %>
                    <th><%= title %></th>
                <% end %>
            </tr>
        </thead>
        <tbody>
            <% @purchase_orders_list.each do |entry| %>
                <tr data-link="<%= purchase_order_path(entry["id"]) %>">
                    <% PurchaseOrder.column_names.each do |column| %>
                        <td><%= entry[column] %></td>
                    <% end %>
                </tr>
            <% end %>
        </tbody>
    </table>
<% end %>

<div id="purchaser-table">
<%= render 'purchasers' %>
</div>
